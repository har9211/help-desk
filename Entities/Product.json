{
  "name": "Product",
  "type": "object",
  "properties": {
    "name": {
      "type": "string",
      "description": "Product name"
    },
    "description": {
      "type": "string",
      "description": "Product description"
    },
    "category": {
      "type": "string",
      "enum": [
        "stationary",
        "clothes",
        "electronics",
        "vegetables",
        "footwear",
        "watches",
        "groceries",
        "other"
      ],
      "description": "Product category"
    },
    "price": {
      "type": "number",
      "description": "Product price"
    },
    "seller_name": {
      "type": "string",
      "description": "Name of the seller"
    },
    "seller_contact": {
      "type": "string",
      "description": "Seller contact information"
    },
    "image_url": {
      "type": "string",
      "description": "Product image URL"
    },
    "is_available": {
      "type": "boolean",
      "default": true,
      "description": "Product availability"
    },
    "location": {
      "type": "string",
      "description": "Seller location"
    }
  },
  "required": [
    "name",
    "description",
    "category",
    "price",
    "seller_name",
    "seller_contact"
  ]
}